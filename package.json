  },
  "main": "dist/vue-components.umd.js",
  "module": "dist/vue-components.es.js",
  "types": "types/src",
  "exports": {
    ".": {
      "import": "./dist/vue-components-test.es.js",
      "require": "./dist/vue-components-test.umd.js"
    }
  },
  "files": [
    "dist",
    "types"
  ],
