    "animejs": "^3.2.1",
    "jest": "^26.6.3",
    "postcss": "^8.2.10",
    "postcss-import": "^14.0.1",
    "postcss-nested": "^5.0.5",
    "tailwindcss": "^2.1.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.3.2",
